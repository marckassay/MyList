{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "vueIndentScriptAndStyle": false,
  "useTabs": false,
  "overrides": [
    {
      "files": [
        "package.json",
        "tsconfig.*.json",
        ".swcrc",
        ".prettierrc",
        ".eslintrc.json"
      ],
      "options": { "parser": "json" }
    },
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": ["*.tsx", "*.jsx"],
      "options": {
        "parser": "typescript",
        "bracketSameLine": true
      }
    }
  ]
}
